<?xml version="1.0" encoding="utf-8" ?>
<ContentPage
    xmlns:forms1="using:Xamarin.Forms" xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
               xmlns:forms="clr-namespace:AlphaTab.Platform.CSharp.Xamarin.Forms;assembly=AlphaTab"
             x:Class="Scalex.Views.TablaturePage">
    <ContentPage.Content>
        <StackLayout>
            <Picker x:Name="trackPicker" SelectedIndexChanged="trackPicker_SelectedIndexChanged" ></Picker>
            <forms:AlphaTab x:Name="AlphaTabControl"
                    HorizontalOptions="Fill"
                    VerticalOptions="Fill"
                    BackgroundColor="White" />
        </StackLayout>
    </ContentPage.Content>
</ContentPage>